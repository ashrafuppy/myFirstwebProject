pipeline {
    agent any

    stages {
        stage('Dev') {
            steps {
                echo 'Hello Dev'
            }
        }
        stage('Inte') {
            steps {
                echo 'Hello Inte'
            }
        }
        parameters {
            string 'USERNAME'
            password defaultValue: '', name: 'Password'
        }
        stage('QA') {
            steps {
                echo 'Hello QA'
            }
        }
        stage('REG') {
            steps {
                echo 'Hello REGR'
            }
        }
    }
}
